<%- include('includes/head.ejs') %>
    <link rel="stylesheet" href="/css/product.css">
</head>

<body>
    <%- include('includes/navigation.ejs') %>

    <main>
        <% if (users.length > 0) { %>
            <h3>List of Users</h3>
            <ul>
            <% for (let user of users) { %>
                <li><h4><%= user.title %></h4></li>
            <% } %>
            </ul>
        <% } else { %>
            <h3>No User Found!</h3>
        <% } %>
    </main>
<%- include('includes/end.ejs') %>